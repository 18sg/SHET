
_shet()
{
	local curr
	curr="${COMP_WORDS[COMP_CWORD]}"
	head=$(echo "$curr" | sed 's:^.*/::')
	tail=$(echo "$curr" | grep -o '^.*/')
	available=$(shet -l /meta/ls $tail | sed -e 's\:dir$\/\' -e 's/:.*$/\ /')
	COMPREPLY=( $(compgen -W "${available}" -- ${head} |
	sed "s:\(^.*$\):$tail\1:") )
	compopt -o nospace shet
}
 
complete -o nospace -F _shet shet
